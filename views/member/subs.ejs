<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>GOES AGAINST</title>
	<link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
	<a href="/member/<%= member._id %>">BACK</a>

	<% if (member._id.equals(user && user._id)) { %>
		<h3>SUBSCRIBERS</h3>

		<% subs.forEach(function(sub) { %>
			<form action="/subs/<%= sub._id %>?_method=DELETE" method="POST">
				<input type="submit" value="X">
				<%= sub.subEmail %>
			</form>
		<% }) %>

		<% if (subs.length === 0) { %>
			<p>No subscribers yet! :^(</p>
		<% } %>
	<% } %>
</body>

</html>